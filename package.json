{
  "name": "ng-yandex-metrika",
  "version": "5.1.1",
  "description": "Yandex Metric for Angular",
  "keywords": [
    "yandex",
    "metrika",
    "ng",
    "angular"
  ],
  "author": "Lyubimov Roman, Nikolay Galko",
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.10",
    "@angular/core": "^14.2.10",
    "typescript": "^4.8.4"
  },
  "repository": {
    "originalUrl": "git@github.com:FriOne/ng-yandex-metrika.git",
    "url": "git@github.com:galek/ng-yandex-metrika.git"
  },
  "scripts": {
    "clean:nix": "rm -rf node_modules && rm -rf package-lock.json && rm -rf dist && rm -rf build",
    "build": "rimraf dist && tsc -p tsconfig.lib.json"
  },
  "sideEffects": false,
  "type": "module",
  "module": "dist/lib/index.js",
  "es2021": "dist/lib/index.js",
  "es2020": "dist/lib/index.js",
  "typings": "src/lib/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./dist/lib/index.js",
      "es2020": "./dist/lib/index.js",
      "es2015": "./dist/lib/index.js",
      "node": "./dist/lib/index.jss",
      "default": "./dist/lib/index.js"
    },
    "./dist": "./dist",
    "./dist/lib": "./dist/lib"
  },
  "devDependencies": {
    "rimraf": "4.0.5"
  }
}
